  "repository": {
    "type": "git",
    "url": "git://github.com/krakenjs/meddleware.git"
  },
  "bugs": {
    "url": "https://github.com/krakenjs/meddleware/issues"
  },
  "homepage": "https://github.com/krakenjs/meddleware"
